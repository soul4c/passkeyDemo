	margin-left: 95px;
}
.banner-box{
	position: absolute;
	top: 0;
	left: 235px;
	background: #f7f7f7;
	border: solid 1px #118855;
	width: 699px;
	z-index: 200;
	cursor: default;
	display: none;
}
.bnav1{
	height: 435px;
}
.bnav1>.arrow{
	position: absolute;
	left: -8px;
	top: 35px;
}
.bnav2{
	height: 340px;
}
.bnav2>.arrow{
	position: absolute;
	left: -8px;
	top: 121px;
}
.bnav3{
	height: 401px;
}
.bnav3>.arrow{
	position: absolute;
	left: -8px;
	top: 205px;
}
.bnav4{
	height: 340px;
}
.bnav4>.arrow{
	position: absolute;
	left: -8px;
	top: 291px;
}
.banner-nav>li:hover{
	background-color: #118855;
}
.banner-nav>li:first-of-type:hover .bnav1{
	display: block;
}
.banner-nav>li:nth-of-type(2):hover .bnav2{
	display: block;
}
.banner-nav>li:nth-of-type(3):hover .bnav3{
	display: block;
}
.banner-nav>li:last-of-type:hover .bnav4{
	display: block;
}

