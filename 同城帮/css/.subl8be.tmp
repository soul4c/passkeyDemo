/*统一样式*/
html,div,ul,li,p,span,img,form,input,a,ol,dl,dt,dd,table,tr,td,body{
	margin: 0;
	padding: 0;
	color: #666666;
	font-family: "微软雅黑";
	box-sizing:border-box; 
}
ul,li{
	list-style-type: none;
}
a{
	text-decoration: none;
}
.close{
	cursor: pointer;
	border: solid 1px #d2d2d2;
	border-radius: 3px;
	display: block;
	width: 55px;
	text-align: center;
	height: 21px;
	line-height: 21px;
	font-size: 13px;
	font-weight: 400;
}






/*头部导航栏样式*/
#index-nav{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 33px;
	border-bottom: 1px solid #c7c7c7;
	background: url(../img/t011da1a195c36e744d.png)left top repeat-x #f7f7f7;
}
#index-nav-box{
	margin: 0 auto;
	width: 1200px;
	height: 32px;
	padding-top: 8px;
	position: relative;
}
#index-nav-box li{
	font-size: 12px;
	float: left;
	height: 18px;
	padding: 0 12px;
	border-right: 1px solid #d2d2d2;
}
#index-nav-box>li:first-of-type{
	padding-left: 0;
}
#index-nav-box>li:nth-of-type(2){
	border: none;
}
#index-nav-box>li:nth-of-type(2)>a{
	text-decoration: none;
}
#index-nav-box>li:nth-of-type(2)>a:hover{
	text-decoration: underline;
}
#index-city{
	/*width: 108px;*/
	padding-left: -12;
}
#index-city>span:first-of-type{
	color: #118855;
	/*padding: 0 5px 0 0;*/
	font-size: 13px;
	font-weight: bold;
}
#index-city>span:last-of-type:hover{
	cursor: pointer;
	text-decoration: underline;
}
#changecity{
	width: 460px;
	height: 190px;
	position: absolute;
	left: 33px;
	top: 26px;
	background: #fbfbfb;
	border: solid 1px #d2d2d2;
	display: none;
	z-index: 500;
}
#changecity>span:first-of-type{
	margin-left: 10px;
	margin-top: 10px;
	font-size: 13px;
	height: 20px;
	line-height: 20px;	
	display: block;
	float: left;
	color: #000000;
}
#changecity>span:last-of-type{
	position: absolute;
	right: 10px;
	top: 10px;
}
#city-box{
	width: 438px;
	height: 143px;
	border-top: 2px solid #ebebeb;
	border-bottom: 2px solid #ebebeb;
	border-left: 1px solid #ebebeb;
	border-right: 1px solid #ebebeb;
	background: #fff;
	position: absolute;
	left: 10px;
	top: 35px;
}
#index-nav-boxright{
	float: right;
}
#index-nav-boxright>li:last-of-type{
	border:  none;
}
#index-nav-boxright>li:last-of-type>span{
	color: #ff0000;
}
#index-nav-boxright>li:hover>span,#index-nav-boxright>li>a:hover{
	text-decoration: underline;
}
#index-nav-boxright>li>span{
	cursor: pointer;
}
#serve>b{
	display: inline-block;
	width: 7px;
	height: 12px;
	margin-left: 10px;
	background: url(../img/t01548f00668b24011b.gif) 0 -42px no-repeat;
}
#serve>div{
	width: 92px;
	height: 81px;
	background: #fff;
	border: solid 1px #999999;
	position: absolute;
	top: 0;
	right: 123px;
	z-index: 10;
	display:none;
}
#serve>div>p{
	margin-left: 12px;
}
#serve>div>p>b{
	display: inline-block;
	width: 7px;
	height: 12px;
	margin-left: 10px;
	background: url(../img/t01548f00668b24011b.gif) 0 -80px no-repeat;
}
#serve>div>p:first-of-type{
	height: 33px;
	line-height: 32px;
}
#serve>div>p:nth-of-type(2),#serve>div>p:nth-of-type(3){
	line-height: 22px;
}
#serve>div>p:nth-of-type(2)>a:hover,#serve>div>p:nth-of-type(3)>a:hover{
	text-decoration: underline;
}
#serve:hover div{
	display: block;
}




