/*轮播图，侧栏导航*/
.banner-img{
	width: 1200px;
	height: 340px;
	overflow: hidden;
}
.banner{
	width: 1200px;
	height: 340px;
	margin: 0 auto;
	/*background: #a00;*/
	position: relative;
}
.banner-nav{
	height: 340px;
	width: 235px;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 3;
	background: url(../img/t01c035e0988a964a21.png) left top repeat;
}
.banner-nav>li{
	height: 85px;
	width: 235px;
	cursor: pointer;
	border-bottom: 1px solid #9a9895;
	/*position: relative;*/
}
.banner-nav>li:first-of-type{
	background: url(../img/t01280de86e9d9dc470.png) 0 -9px no-repeat;
}
.banner-nav>li:nth-of-type(2){
	background: url(../img/t01280de86e9d9dc470.png) 0 -104px no-repeat;
}
.banner-nav>li:nth-of-type(3){
	background: url(../img/t01280de86e9d9dc470.png) 0 -207px no-repeat;
}
.banner-nav>li:last-of-type{
	border: none;
	background: url(../img/t01280de86e9d9dc470.png) 0 -308px no-repeat;
}
.banner-nav>li>span{
