}
.otherlogul>li:nth-of-type(3){
	background: url(../img/sp.png) -400px -330px no-repeat;
}
.otherlogul>li:nth-of-type(3):hover{
	background: url(../img/sp.png) -430px -330px no-repeat;
}
.otherlogul>li:last-of-type{
	background: url(../img/sp.png) -400px -380px no-repeat;
}
.otherlogul>li:last-of-type:hover{
	background: url(../img/sp.png) -430px -380px no-repeat;
}
#login-box-bot{
	height: 39px;
	line-height: 39px;
	position: absolute;
	bottom: 0;
	left: 0;
}
#login-box-bot>a{
	font-size: 12px;
	padding: 0 13px;
}
#login-box-bot>a:hover span{
	text-decoration: underline;
}
#login-box-bot>a:first-of-type{
	color: #999999;
	margin-left: 144px;
	border-right: solid 1px #999;
}
#blue{
